{
  "name": "happyprism",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "react-scripts build",
    "heroku-postbuild": "npm install && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run watch\" \"nodemon\"",
    "watch": "watchify src/index.js -o public/bundle.js --debug --verbose",
    "build:prod": "browserify src/index.js | uglifyjs -cm > public/bundle.min.js",
    "start-client": "react-scripts start",
    "deploy": "npm run build && git add . && git commit -m 'deploy' && git push origin main && heroku open"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@google/generative-ai": "^0.14.1",
    "@mui/material": "^5.16.0",
    "babel": "^6.23.0",
    "date-fns": "^3.6.0",
    "emoji-picker-react": "^4.11.1",
    "express": "^4.19.2",
    "lucide-react": "^0.397.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "recharts": "^2.12.7",
    "sentiment": "^5.0.2",
    "showdown": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.9.5",
    "uglify-js": "^3.17.4",
    "watchify": "^4.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ]
    ]
  },
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
