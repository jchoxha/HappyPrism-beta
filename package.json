{
  "name": "happyprism",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "browserify public/index.js -o public/bundle.js",
    "watch": "watchify public/index.js -o public/bundle.js --debug --verbose",
    "build:prod": "browserify public/index.js | uglifyjs -cm > public/bundle.min.js",
    "start": "nodemon",
    "dev": "concurrently \"npm run watch\" \"nodemon\""
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@google/generative-ai": "^0.12.0",
    "@radix-ui/react-progress": "^1.1.0",
    "express": "^4.19.2",
    "lucide-react": "^0.397.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "sentiment": "^5.0.2",
    "showdown": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "uglify-js": "^3.17.4",
    "watchify": "^4.0.0"
  },
  "author": "",
  "license": "ISC"
}
